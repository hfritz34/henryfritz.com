{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico,svg}",
      "/css/*",
      "/assets/*",
      "/*.{ico,png}"
    ]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".pdf": "application/pdf"
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=0"
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
} 